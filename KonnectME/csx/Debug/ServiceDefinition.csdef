<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="KonnectME" schemaVersion="2014-06.2.4" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="WebRole1">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings />
    <Endpoints>
      <InputEndpoint name="Microsoft.WindowsAzure.Plugins.WebDeploy.InputEndpoint" protocol="tcp" port="8172" localPort="8172" />
      <InputEndpoint name="Web Endpoint" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <Certificates />
  </WebRole>
  <WorkerRole name="WorkerRole">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings />
    <Endpoints>
      <InputEndpoint name="Media_Endpoint" protocol="udp" port="11000" localPort="11000" />
    </Endpoints>
    <Certificates />
  </WorkerRole>
</ServiceDefinition>